{
    "name": "@entity-routes/test-utils",
    "version": "0.1.0",
    "description": "Test utilities for entity-routes.",
    "source": "src/index.ts",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "scripts": {
        "test": "jest --runInBand --forceExit",
        "test:clear": "jest --clearCache",
        "test:watch": "jest --runInBand --forceExit --watch --no-watchman",
        "test:coverage": "jest --coverage --runInBand --forceExit",
        "dev": "ttsc --watch --incremental -p tsconfig.build.json --outDir dist/esm",
        "build:cjs": "ttsc -p tsconfig.build.json --outDir dist/cjs -m CommonJS",
        "build:esm": "ttsc -p tsconfig.build.json --outDir dist/esm",
        "build": "concurrently yarn:build:*",
        "prebuild": "rimraf dist"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/astahmer/entity-routes.git"
    },
    "author": "Alexandre Stahmer <alexandre.stahmer@gmail.com>",
    "license": "ISC",
    "dependencies": {
        "@entity-routes/core": "^0.1.8",
        "typeorm": "^0.2.29"
    },
    "devDependencies": {
        "axios": "^0.21.0"
    }
}
