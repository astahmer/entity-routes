{
    "name": "@astahmer/entity-routes",
    "version": "0.1.0",
    "description": "Design your API around your entities in minutes",
    "source": "src/index.ts",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "unpkg": "dist/index.umd.js",
    "types": "dist/index.d.ts",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "test": "jest --runInBand --detectOpenHandles",
        "test:clear": "jest --clearCache",
        "test:watch": "jest --runInBand --detectOpenHandles --watch --no-watchman",
        "test:coverage": "jest --coverage --runInBand --detectOpenHandles",
        "tsc": "tsc",
        "commit": "git-cz",
        "prettier": "prettier --write \"./{src,tests}/**/*.ts\"",
        "dev": "webpack --env.hmr",
        "build": "webpack --env.prod",
        "watch": "webpack --env.prod --env.hmr",
        "start": "node -r ts-node/register dist/index",
        "release": "standard-version"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "dependencies": {
        "@astahmer/entity-validator": "^0.1.1",
        "@types/body-parser": "^1.19.0",
        "class-validator": "^0.12.2",
        "reflect-metadata": "^0.1.13",
        "typedi": "^0.8.0",
        "typeorm": "^0.2.25",
        "uuid": "^8.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.10.5",
        "@babel/plugin-proposal-optional-chaining": "^7.10.3",
        "@babel/preset-env": "^7.10.4",
        "@types/jest": "^26.0.3",
        "@types/koa": "^2.11.3",
        "@types/koa-bodyparser": "^4.3.0",
        "@types/koa-router": "^7.4.1",
        "@types/uuid": "^8.0.0",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "axios": "^0.19.2",
        "babel-loader": "^8.1.0",
        "body-parser": "^1.19.0",
        "clean-webpack-plugin": "^3.0.0",
        "commitizen": "^4.1.2",
        "express": "^4.17.1",
        "husky": "^4.2.5",
        "jest": "^26.1.0",
        "jest-diff": "^26.1.0",
        "jest-extended": "^0.11.5",
        "koa": "^2.13.0",
        "koa-bodyparser": "^4.3.0",
        "koa-router": "^9.0.1",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "sql.js": "^1.3.0",
        "standard-version": "^8.0.0",
        "ts-jest": "^26.1.1",
        "ts-loader": "^7.0.5",
        "ts-node": "^8.10.2",
        "tsconfig-paths": "^3.9.0",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "typescript": "^3.9.5",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12",
        "webpack-loader-helper": "^1.0.0",
        "webpack-node-externals": "^1.7.2"
    },
    "keywords": [
        "entity",
        "routes",
        "entity-routes",
        "api",
        "typescript",
        "model",
        "typeorm",
        "koa"
    ],
    "author": "Alexandre Stahmer <alexandre.stahmer@gmail.com>",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/astahmer/entity-routes.git"
    }
}
