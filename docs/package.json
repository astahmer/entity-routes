{
    "name": "@entity-routes/docs",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "docs:clean": "rimraf $npm_package_directories_typedoc",
        "docs:make": "typedoc $npm_package_directories_typedocEntrypoint",
        "docs:links": "ts-node $npm_package_directories_checkLinks",
        "docs:fix": "ts-node $npm_package_directories_docFix",
        "docs:fixdry": "ts-node $npm_package_directories_docFix --dry",
        "docs:build": "npm run docs:clean && npm run docs:make && npm run docs:fix",
        "deploy": "npm run docs:build && vercel",
        "prettier": "prettier --write \"./{src,scripts}/**/*.{js,jsx,ts,tsx}\"",
        "postinstall": "patch-package"
    },
    "directories": {
        "typedoc": "./src/pages/definitions",
        "typedocEntrypoint": "../src/index.ts",
        "docFix": "./scripts/fixDocs.ts",
        "checkLinks": "./scripts/checkLinks.ts"
    },
    "dependencies": {
        "@chakra-ui/icons": "^1.0.1",
        "@chakra-ui/react": "^1.0.1",
        "@emotion/react": "^11.1.1",
        "@emotion/styled": "^11.0.0",
        "@mdx-js/react": "^1.6.22",
        "framer-motion": "^2.9.5",
        "github-slugger": "^1.3.0",
        "next": "^10.0.3",
        "next-seo": "^4.17.0",
        "parse-numeric-range": "^1.2.0",
        "patch-package": "^6.2.2",
        "prism-react-renderer": "^1.1.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-icons": "^4.1.0",
        "remark-emoji": "^2.1.0"
    },
    "devDependencies": {
        "@mdx-js/loader": "^1.6.22",
        "@types/github-slugger": "^1.3.0",
        "@types/node": "^14.14.10",
        "@types/react": "^16.14.2",
        "chalk": "^4.1.0",
        "compose-function": "^3.0.3",
        "consola": "^2.15.0",
        "directory-tree": "^2.2.5",
        "front-matter": "^4.0.2",
        "lodash": "^4.17.20",
        "lodash.throttle": "^4.1.1",
        "markdown-link-check": "^3.8.3",
        "markdown-toc": "^1.2.0",
        "next-images": "^1.6.2",
        "next-mdx-enhanced": "^5.0.0",
        "prettier": "^2.2.1",
        "prettier-plugin-sorted": "^2.0.0",
        "remark-mdx-metadata": "^1.0.1",
        "remark-slug": "^6.0.0",
        "replace-in-file": "^6.1.0",
        "shelljs": "^0.8.4",
        "smart-glob": "^1.0.1",
        "ts-node": "^9.0.0",
        "typedoc": "^0.17.0-3",
        "typedoc-plugin-markdown": "^2.4.2",
        "typescript": "^4.1.2"
    }
}
